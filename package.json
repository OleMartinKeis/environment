{
  "name": "olemartinkeis-joker",
  "version": "0.1.1",
  "description": "Application that tells jokes",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "test": "node src/js/jokes.js",
    "format": "prettier -w src/js/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OleMartinKeis/joker.git"
  },
  "keywords": [
    "jokes",
    "test"
  ],
  "author": "Ole Martin Keiseraas, keisero@outlook.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/OleMartinKeis/joker/issues"
  },
  "homepage": "https://github.com/OleMartinKeis/joker#readme",
  "devDependencies": {
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4"
  },
  "dependencies": {
    "give-me-a-joke": "^0.5.1"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
